<acsconfig xmlns="http://openncip.org/acs-config/1.0/">
<!-- above address gets nothing, it's just a namespace -->
<error-detect enabled="true" />

<!--
Set Net::Server::PreFork runtime parameters
syslog_ident will identify SIP2 Koha server entries in syslog
For OpenSolaris, add: syslog_logsock=stream
-->
<server-params
min_servers='10'
min_spare_servers='2'
max_servers='150'
log_file='Sys::Syslog'
syslog_ident='koha_sip'
syslog_facility='local6'
/>

<listeners>
<!-- vestigial HTTP, never implemented: just use the OPAC!
<service
port="0:8080/tcp"
transport="http"
protocol="NCIP/1.0" />
-->
<service
port="8023/tcp"
transport="telnet"
protocol="SIP/2.00"
timeout="60" />

<service
port="23.253.104.41:6001/tcp"
transport="RAW"
protocol="SIP/2.00"
timeout="60" />

<service
port="23.253.104.41:6002/tcp"
transport="RAW"
protocol="SIP/2.00"
timeout="60" />

<service
port="23.253.104.41:6006/tcp"
transport="RAW"
protocol="SIP/2.00"
timeout="60" />

<service
port="23.253.104.41:6008/tcp"
transport="RAW"
