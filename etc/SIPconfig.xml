<accounts>
<login id="sipmai" password="sipchk" delimiter="|" error-detect="enabled" institution="MAIN" terminator="CR" />
<login id="SIPPCR" password="SIPCHK" delimiter="|" error-detect="enabled" institution="MAIN" terminator="CR" />
<login id="SIP360" password="SIPCHK" delimiter="|" error-detect="enabled" institution="MAIN" terminator="CR" />
<login id="SIPMUR" password="SIPCHK" delimiter="|" error-detect="enabled" institution="MURPHY" terminator="CR" />
<login id="SIPMON" password="SIPCHK" delimiter="|" error-detect="enabled" institution="MONROVIA" terminator="CR" />
<login id="SIPHAZ" password="SIPCHK" delimiter="|" error-detect="enabled" institution="HAZELGREEN" terminator="CR" />
<login id="SIPOVD" password="SIPCHK" delimiter="|" error-detect="enabled" institution="MAIN" terminator="CR" />
<login id="sipmad" password="sipchk" delimiter="|" error-detect="enabled" institution="MADISON" terminator="CR" />
<login id="sipbai" password="sipchk" delimiter="|" error-detect="enabled" institution="BAILEYCOVE" terminator="CR" />
</accounts>

<!--
Institution tags are for enabled branches. There needs to be one
institution stanza for each institution named in the accounts above.
The implementation attribute is actually used to find the code to run,
in our case "ILS".
-->

<institutions>
<institution id="MAIN" implementation="ILS" parms="">
<policy checkin="true" renewal="true" checkout="true"
status_update="false" offline="false"
timeout="100"
retries="5" />
</institution>
<institution id="MADISON" implementation="ILS" parms="">
<policy checkin="true" renewal="true" checkout="true"
status_update="false" offline="false"
timeout="100"
retries="5" />
</institution>
<institution id="BAILEYCOVE" implementation="ILS" parms="">
<policy checkin="true" renewal="true" checkout="true"
status_update="false" offline="false"
timeout="100"
retries="5" />
</institution>
<institution id="MONROVIA" implementation="ILS" parms="">
<policy checkin="true" renewal="true" checkout="true"
status_update="false" offline="false"
timeout="100"
retries="5" />
</institution>
<institution id="MURPHY" implementation="ILS" parms="">
<policy checkin="true" renewal="true" checkout="true"
status_update="false" offline="false"
timeout="100"
retries="5" />
</institution>
<institution id="HAZELGREEN" implementation="ILS" parms="">
<policy checkin="true" renewal="true" checkout="true"
status_update="false" offline="false"
timeout="100"
retries="5" />

</institution>
</institutions>
</acsconfig>
